pipeline {
    agent any

    stages {
        stage('unitTest ') {
            steps {
                echo 'Hello Unit test is ok '
            }
	}
        stage('Build ') {
            steps {
                echo 'Hello Build is ok'
        
            }
	}
        stage('Deploy ') {
            steps {
                echo 'Hello Deploy to test is ok '
            }
	}
        stage('DeploytoProd ') {
            steps {
                echo 'Hello DeploytoProd is ok '
            }
        }
    }
}
